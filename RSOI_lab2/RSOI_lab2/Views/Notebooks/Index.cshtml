@{
    var S = RSOI_lab2.Controllers.NotebooksController.SpisokN;
}

@{
    ViewBag.Title = "Ноутбуки";
}

<h2>Ноутбуки</h2>

<table border="1" width="800" bgcolor="#CFCFCF">
    <tr>
        <td>ID</td>
        <td>Наименование</td>
        
    </tr>
    @{ foreach (var item in S)
     {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
        </tr>
     } }
</table>



